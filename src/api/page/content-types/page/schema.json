{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "private": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": false,
      "regex": "^[a-z0-9/]+(?:-[a-z0-9/]+)*$"
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.typed-title",
        "sections.mdx-block",
        "sections.carousel",
        "sections.services",
        "sections.cta-numbers",
        "sections.faq",
        "sections.categories",
        "sections.game-request",
        "sections.media-grid",
        "sections.media-carousel",
        "sections.map",
        "sections.contact"
      ]
    },
    "metadata": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "seo.seo",
      "required": true
    }
  }
}
